<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayoutself1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/RelativeLayoutself2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:orientation="vertical"
        android:padding="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">
        <!--<TextView-->
        <!--android:id="@+id/tv"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content" />-->


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/relativeFrame"
            android:layout_toStartOf="@+id/checkbox">

        <ImageView
            android:id="@+id/byselfimage"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerVertical="true"
            android:contentDescription="@string/app_name" />

        <RelativeLayout
            android:id="@+id/RelativeLayoutself3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage"
            android:visibility="visible">

            <TextView
                android:id="@+id/byselftitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/title"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehicletitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselftitle"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutcategory"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutself3"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage"
            android:visibility="visible">

            <TextView
                android:id="@+id/byselfcategory"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/category"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehiclecategory"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfcategory"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutbrand"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutcategory"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/byselfbrand"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/brand"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehiclebrand"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfbrand"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutself4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutbrand"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/byselfmodel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/model"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehiclemodel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfmodel"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutself5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutself4"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/byselfrtocity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/city"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehiclertocity"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfrtocity"
                android:text="- "
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutself6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutself5"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/byselfcolor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/color"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehiclecolor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfcolor"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutself7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutself6"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/byselfreg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/no_register"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehiclereg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfreg"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutself8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutself7"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/byselfrepo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/repodate"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehiclerepo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfrepo"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutself9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutself8"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/byselfyard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/yard_rent_per_day"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehicleyard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/byselfyard"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/RelativeLayoutself10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RelativeLayoutself9"
            android:layout_marginStart="5dp"
            android:layout_toEndOf="@+id/byselfimage">

            <TextView
                android:id="@+id/manufactureyeartext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/manufacture_year"
                android:textColor="#848484"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/vehicle_manu_year"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/manufactureyeartext"
                android:text="-"
                android:layout_marginStart="5dp"
                android:textColor="#848484"
                android:textSize="12sp" />

        </RelativeLayout>
        </RelativeLayout>


        <CheckBox
            android:id="@+id/checkbox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true" />


    </RelativeLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/RelativeLayoutself2"
        android:background="@color/grey" />
</RelativeLayout>