                        android:textColor="#808080" />

                    <RelativeLayout
                        android:id="@+id/relgoing"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/typetext">

                        <TextView
                            android:id="@+id/goingtext"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="5dip"
                            android:text="@string/going_count"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_going_cnt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@+id/goingtext"
                            android:layout_toEndOf="@+id/goingtext"
                            android:text="-"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/relignore"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/relgoing">

                        <TextView
                            android:id="@+id/ignoretext"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
