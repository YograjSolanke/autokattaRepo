                    android:visibility="gone">

                    <TextView
                        android:id="@+id/auctitletext"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:text="@string/auction_title"
                        android:textColor="#808080"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_auc_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@+id/auctitletext"
                        android:text="-"
                        android:textColor="#808080" />

                    <TextView
                        android:id="@+id/vehiclenotext"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/auctitletext"
                        android:layout_marginTop="5dip"
                        android:text="@string/no_of_vehicle"
                        android:textColor="#808080"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_auc_noofvehicle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/vehiclenotext"
                        android:layout_toEndOf="@+id/vehiclenotext"
                        android:text="-"
                        android:textColor="#808080" />

                    <TextView
                        android:id="@+id/enddatetext"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/vehiclenotext"
