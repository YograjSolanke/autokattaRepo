                            android:layout_toEndOf="@+id/ignoretext"
                            android:text="-"
                            android:textColor="#808080" />

                    </RelativeLayout>
                </RelativeLayout>

                <!--Post Relative -->

                <RelativeLayout
                    android:id="@+id/postrel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
