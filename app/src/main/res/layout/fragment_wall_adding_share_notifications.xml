                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_auc_enddate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/enddatetext"
                        android:layout_toEndOf="@+id/enddatetext"
                        android:text="-"
                        android:textColor="#808080" />

                    <TextView
                        android:id="@+id/endtimetext"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/enddatetext"
                        android:layout_marginTop="5dip"
                        android:text="@string/end_time"
                        android:textColor="#808080"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_auc_endtime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/endtimetext"
                        android:layout_toEndOf="@+id/endtimetext"
                        android:text="-"
                        android:textColor="#808080" />

                    <TextView
                        android:id="@+id/typetext"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/endtimetext"
                        android:layout_marginTop="5dip"
                        android:text="@string/type_of_auction"
