<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <android.support.v7.widget.CardView
        android:id="@+id/share_card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/layout_top"
        android:layout_marginTop="@dimen/layout_top"
        android:foreground="?attr/selectableItemBackground">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/relative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/relImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/user_pro_pic"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="7dp"
                        android:contentDescription="@string/app_name"
                        android:src="@mipmap/profile" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relContact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/relImg">

                    <TextView
                        android:id="@+id/share_action_names"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginTop="10dp"
                        android:text="Auction Actions"
                        android:textColor="#2C3343" />

                    <TextView
                        android:id="@+id/share_action_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/share_action_names"
                        android:layout_marginStart="7dp"
                        android:text="Time"
                        android:textColor="#2C3343" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rating_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_toEndOf="@+id/relContact">

                    <ImageButton
                        android:id="@+id/share_favourite"
                        style="?android:attr/borderlessButtonStyle"
                        android:layout_width="@dimen/cards_button_width"
                        android:layout_height="@dimen/card_title_height"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/text_margin_top"
                        android:contentDescription="@string/app_name"
                        android:src="@mipmap/heart_liked"
                        android:visibility="gone" />

                    <ImageButton
                        android:id="@+id/share_unfavourite"
                        style="?android:attr/borderlessButtonStyle"
                        android:layout_width="@dimen/cards_button_width"
                        android:layout_height="@dimen/card_title_height"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/text_margin_top"
                        android:contentDescription="@string/app_name"
                        android:src="@mipmap/ic_heart_like"
                        android:visibility="visible" />

                </RelativeLayout>
            </RelativeLayout>

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/relative"
                android:layout_marginTop="5dp"
                android:background="#cccccc" />

            <LinearLayout
                android:id="@+id/linear_card"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/view1"
                android:layout_marginStart="@dimen/margin_small"
                android:layout_marginTop="@dimen/card_margin"
                android:orientation="vertical"
                android:padding="@dimen/margin_small">

                <RelativeLayout
                    android:id="@+id/mainrel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/sharedataview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:text="data" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/profilerel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:visibility="gone">

                    <RelativeLayout
                        android:id="@+id/profilenamerel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/profilenametxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/name"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_profile_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/profilenametxt"
                            android:hint="@string/name"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profilelocationrel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/profilewebsiterel">

                        <TextView
                            android:id="@+id/profiletypetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/location"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_profilelocation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/profiletypetxt"
                            android:hint="@string/location"
                            android:textColor="#808080" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profileworkrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/profilenamerel">

                        <TextView
                            android:id="@+id/profilelocationtxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/worked_at"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_profileworkat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/profilelocationtxt"
                            android:hint="@string/worked_at"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profilewebsiterel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/profileworkrel">

                        <TextView
                            android:id="@+id/profilewebsitetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/website"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_profilewebsite"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/profilewebsitetxt"
                            android:hint="@string/website"
                            android:textColor="#808080" />

                    </RelativeLayout>
                </RelativeLayout>

                <!-- Store Relative -->

                <RelativeLayout
                    android:id="@+id/storerel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <RelativeLayout
                        android:id="@+id/storenamerel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/storenametxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/store_name"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_storename"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/storenametxt"
                            android:hint="@string/name"
                            android:textColor="#808080"
                            android:textColorHint="#848484" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/storetyperel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/storenamerel">

                        <TextView
                            android:id="@+id/storetypetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/type_of_store"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_storetype"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/storetypetxt"
                            android:hint="@string/type"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/storelocationrel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/storeworkdayrel">

                        <TextView
                            android:id="@+id/storelocationtxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/location"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_location"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/storelocationtxt"
                            android:hint="@string/location"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/storewebsiterel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/storetyperel">

                        <TextView
                            android:id="@+id/storewebsitetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/website"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_website"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/storewebsitetxt"
                            android:hint="@string/website"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/storetimerel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/storewebsiterel">

                        <TextView
                            android:id="@+id/storetimetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/timing"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_timing"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/storetimetxt"
                            android:hint="@string/timing"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/storeworkdayrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/storetimerel">

                        <TextView
                            android:id="@+id/storeworkdaytxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/working_day"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_workday"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/storeworkdaytxt"
                            android:hint="@string/working_day"
                            android:textColor="#808080" />
                    </RelativeLayout>
                </RelativeLayout>

                <!-- Product Relative -->

                <RelativeLayout
                    android:id="@+id/productrel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <RelativeLayout
                        android:id="@+id/productnamerel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/productnametxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/product_name"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_productname"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/productnametxt"
                            android:hint="@string/names"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/producttyperel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/productnamerel">

                        <TextView
                            android:id="@+id/producttypetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/product_type"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_producttype"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/producttypetxt"
                            android:hint="@string/type"
                            android:textColor="#808080" />

                    </RelativeLayout>
                </RelativeLayout>

                <!-- Service Relative -->
                <RelativeLayout
                    android:id="@+id/servicerel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <RelativeLayout
                        android:id="@+id/servicenamerel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/servicenametxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/service_name"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_servicename"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/servicenametxt"
                            android:hint="@string/names"
                            android:textColor="#808080" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/servicetyperel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/servicenamerel">

                        <TextView
                            android:id="@+id/servicetypetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/service_type"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_servicetype"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/servicetypetxt"
                            android:hint="@string/type"
                            android:textColor="#808080" />

                    </RelativeLayout>
                </RelativeLayout>

                <!-- Vehicle Relative -->

                <RelativeLayout
                    android:id="@+id/vehiclerel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <RelativeLayout
                        android:id="@+id/vehiclenamerel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/vehiclenametxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/title"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/vehiclenametxt"
                            android:hint="@string/title"
                            android:textColor="#808080" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/pricerel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/vehiclenamerel">

                        <TextView
                            android:id="@+id/pricetxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/price"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_price"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/pricetxt"
                            android:hint="@string/price"
                            android:textColor="#808080" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/brandrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/pricerel">

                        <TextView
                            android:id="@+id/brandtxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/brand"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_brand"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/brandtxt"
                            android:hint="@string/brand"
                            android:textColor="#808080" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/modelrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/brandrel">

                        <TextView
                            android:id="@+id/modeltxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/model"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_model"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/modeltxt"
                            android:hint="@string/model"
                            android:textColor="#808080" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/yearrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/modelrel">

                        <TextView
                            android:id="@+id/yeartxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/year_of_mfg"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_year"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/yeartxt"
                            android:hint="@string/years"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/kmrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/yearrel">

                        <TextView
                            android:id="@+id/kmtxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/kms_hrss"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_km_hrs"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/kmtxt"
                            android:hint="@string/kms_hrss"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/RTOrel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/kmrel">

                        <TextView
                            android:id="@+id/RTOtxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/rto_citys"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_RTO"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/RTOtxt"
                            android:hint="@string/rto_citys"
                            android:textColor="#808080" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/regrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/locationrel">

                        <TextView
                            android:id="@+id/regtxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/registration_no"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_registrationNo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/regtxt"
                            android:hint="@string/number"
                            android:textColor="#808080" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/locationrel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/RTOrel">

                        <TextView
                            android:id="@+id/locationtxt"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text="@string/location"
                            android:textColor="#808080"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/share_vehilocation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/locationtxt"
                            android:hint="@string/location"
                            android:textColor="#808080" />

                    </RelativeLayout>
                </RelativeLayout>

                <!-- Search Relative -->

                <RelativeLayout
                    android:id="@+id/searchrel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:text="@string/category"
                        android:textColor="#808080"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_mysearch_category"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@+id/textView2"
                        android:text="@string/category"
                        android:textColor="#808080" />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/textView2"
                        android:layout_marginTop="5dip"
                        android:text="@string/brand"
                        android:textColor="#808080"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_mysearch_brand"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/textView3"
                        android:layout_toEndOf="@+id/textView3"
                        android:text="@string/brand"
                        android:textColor="#808080" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/textView3"
                        android:layout_marginTop="5dip"
                        android:text="@string/model"
                        android:textColor="#808080"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_mysearch_model"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/textView5"
                        android:layout_toEndOf="@+id/textView5"
                        android:text="@string/model"
                        android:textColor="#808080" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/textView5"
                        android:layout_marginTop="5dip"
                        android:text="@string/price"
                        android:textColor="#808080"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/share_mysearch_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/textView6"
                        android:layout_toEndOf="@+id/textView6"
