<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="autokatta.com.view.MyAutokattaContactsActivity"
    tools:showIn="@layout/activity_my_autokatta_contacts">

    <android.support.design.widget.TabLayout
        android:id="@+id/autokatta_contact_tabs"
        android:layout_width="match_parent"
        android:layout_height="35dip"
        android:background="?attr/colorPrimary"
        android:longClickable="true"
        app:tabGravity="fill"
        app:tabIndicatorColor="@color/white"
        app:tabMode="fixed"
        app:tabSelectedTextColor="@color/white"
        app:tabTextColor="#B0C4DE" />

    <android.support.v4.view.ViewPager
        android:id="@+id/autokatta_contact_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/autokatta_contact_tabs"
        app:layout_behavior="@string/appbar_scrolling_view_behavior" />

</RelativeLayout>
